{
  "name": "transcend-body",
  "version": "1.0.0",
  "main": "server/index.ts",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "npx esbuild server/index.ts --platform=node --external:express --external:dotenv --external:bcrypt --external:express-session --external:memorystore --external:passport --external:passport-local --external:drizzle-orm --external:@libsql/client --external:date-fns --external:zod --external:better-sqlite3 --external:ejs --external:ws --external:nanoid --external:drizzle-zod --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "dotenv drizzle-kit push",
    "seed": "tsx scripts/cleanAndSeed.ts",
    "prestart": "npm run build"
  },
  "dependencies": {
    "@jridgewell/trace-mapping": "^0.3.25",
    "@libsql/client": "^0.14.0",
    "@types/pg": "^8.15.5",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^11.3.0",
    "bootstrap": "^5.3.6",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.2",
    "drizzle-zod": "^0.7.0",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "input-otp": "^1.4.2",
    "memoizee": "^0.4.17",
    "memorystore": "^1.6.7",
    "nanoid": "^5.1.5",
    "nodemon": "^3.1.10",
    "openid-client": "^6.5.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "tailwind-merge": "^2.6.0",
    "tw-animate-css": "^1.2.5",
    "ws": "^8.18.0",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@replit/vite-plugin-cartographer": "^0.2.7",
    "@replit/vite-plugin-runtime-error-modal": "^0.0.3",
    "@tailwindcss/typography": "^0.5.15",
    "@types/bcryptjs": "^3.0.0",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/express": "4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node": "20.16.11",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/ws": "^8.5.13",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.4",
    "esbuild": "^0.25.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8"
  }
}
